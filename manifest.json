{
  "description": "List of online resources for developers.",
  "manifest_version": 2,
  "name": "Online Lab",
  "version": "1.0",
  "homepage_url": "http://www.vincenzodevivo.com",
  "icons": {
    "16": "icons/btn_online_lab.16x16.png",
    "20": "icons/btn_online_lab.20x20.png",
    "24": "icons/btn_online_lab.24x24.png",
    "32": "icons/btn_online_lab.32x32.png",
    "48": "icons/btn_online_lab.48x48.png",
    "64": "icons/btn_online_lab.64x64.png",
    "128": "icons/btn_online_lab.128x128.png"
  },
  "browser_action": {
    "default_icon": "icons/btn_online_lab.32x32.png",
    "default_title": "Online Lab",
    "default_popup": "popup/menu.html"
  },
  "options_ui": {
    "page": "settings/options.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "*://raw.githubusercontent.com/*",
     "webRequest"
  ],
  "applications": {
    "gecko": {
      "id": "firefox-addon-online-lab@vincenzodevivo.com",
      "strict_min_version": "45.0"
    }
  }
}